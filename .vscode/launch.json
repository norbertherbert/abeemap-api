  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/src/server.js",
      "cwd": "${workspaceFolder}"
      //      "skipFiles": [
      //        "<node_internals>/**"
      //      ],
    },
    {
      "name": "Launch via NPM",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "debug"],
      "cwd": "${workspaceFolder}"
      //      "skipFiles": [
      //        "<node_internals>/**"
      //      ],
    }
  ]
}
